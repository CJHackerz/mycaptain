.row
  .col-md-4.col-md-offset-4.form
    %h2.center Welcome Back
    %hr
    = simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
      .form-inputs
        = f.input :email, required: false,label: false, placeholder: "your email" 
        = f.input :password, required: false,label: false, placeholder: "your password"
      %br
      .form-actions
        = f.button :submit, "Log in",class: "btn btn-primary pull-right"
    = render "users/shared/links"
